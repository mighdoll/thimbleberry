{
  "name": "thimbleberry",
  "version": "0.1.3",
  "description": "",
  "type": "module",
  "scripts": {
    "prepublish": "rimraf dist && pnpm run build && pnpm run build:types",
    "build": "esbuild src/thimbleberry.ts --bundle --format=esm  --packages=external --loader:.wgsl=text --sourcemap --outdir=dist",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "cypress-component": "cypress open --component --browser chrome:canary",
    "organize": "organize-imports-cli tsconfig.json",
    "format": "prettier-eslint \"src/**/*.ts\" --write",
    "lint": "eslint ."
  },
  "repository": "github:mighdoll/thimbleberry",
  "main": "./dist/thimbleberry.js",
  "module": "./dist/thimbleberry.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "webgpu",
    "reactive",
    "debug"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@petamoriken/float16": "^3.8.0",
    "@reactively/core": "^0.0.8",
    "@reactively/decorate": "0.0.3",
    "berry-pretty": "^0.0.1",
    "fast-deep-equal": "^3.1.3",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@webgpu/types": "^0.1.32",
    "cypress": "^12.12.0",
    "esbuild": "^0.17.18",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.29.8",
    "wrangler": "^2.20.0"
  }
}
